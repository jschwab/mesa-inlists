  ! starting specifications
    initial_mass = 1.325 ! in Msun units
   
  ! when to stop
    log_center_density_limit = 4

  ! turn off nuclear reactions
    net_rate_factor = 0
    eps_nuc_factor = 0
    dxdt_nuc_factor = 0

/ ! end of controls namelist
