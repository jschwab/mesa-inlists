! this is the master inlist that MESA reads when it starts.

! This file tells MESA to go look elsewhere for its configuration
! info. This makes changing between different inlists easier, by
! allowing you to easily change the name of the file that gets read.

&star_job

    read_extra_star_job_inlist1 = .true.
    extra_star_job_inlist1_name = 'inlist_evolve_model'

    read_extra_star_job_inlist2 = .true.
    extra_star_job_inlist2_name = 'inlist_mass_loss'

    read_extra_star_job_inlist3 = .true.
    extra_star_job_inlist3_name = 'inlist_rotation'

    read_extra_star_job_inlist4 = .true.
    extra_star_job_inlist4_name = 'inlist_diffusion'

    read_extra_star_job_inlist5 = .true.
    extra_star_job_inlist5_name = 'inlist_io'

/ ! end of star_job namelist


&controls

    read_extra_controls_inlist1 = .true.
    extra_controls_inlist1_name = 'inlist_evolve_model'

    read_extra_controls_inlist2 = .true.
    extra_controls_inlist2_name = 'inlist_mass_loss'

    read_extra_controls_inlist3 = .true.
    extra_controls_inlist3_name = 'inlist_rotation'

    read_extra_controls_inlist4 = .true.
    extra_controls_inlist4_name = 'inlist_diffusion'

    read_extra_controls_inlist5 = .true.
    extra_controls_inlist5_name = 'inlist_io'

/ ! end of controls namelist


&pgstar

    read_extra_pgstar_inlist1 = .true.
    extra_pgstar_inlist1_name = 'inlist_pgstar'

/ ! end of pgstar namelist
